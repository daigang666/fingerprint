{
  "name": "@gangdai/fingerprint",
  "version": "1.1.0",
  "description": "A lightweight, zero-dependency browser fingerprint collection library with TypeScript support",
  "keywords": [
    "fingerprint",
    "device-fingerprint",
    "browser-fingerprint",
    "device-identification",
    "canvas-fingerprint",
    "webgl-fingerprint",
    "audio-fingerprint",
    "browser-detection",
    "typescript"
  ],
  "author": "Gang Dai <gang.dai@qq.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup && cp dist/index.browser.js docs/",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daigang666/fingerprint.git"
  },
  "homepage": "https://github.com/daigang666/fingerprint#readme",
  "bugs": {
    "url": "https://github.com/daigang666/fingerprint/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
